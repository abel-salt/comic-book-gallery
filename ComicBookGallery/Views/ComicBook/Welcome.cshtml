@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Welcome screen";

}


<h1>@ViewBag.SeriesTitle</h1>
<h2>Item @ViewBag.IssueNumber</h2>
<h3>Description</h3>
<div> @Html.Raw(ViewBag.Description)</div>
<h3>Artists: </h3>
@if (@ViewBag.Artists.Length > 0)
{

    <ul>
        @foreach (string artist in ViewBag.Artists)
        {
            <li> <pre> @artist </pre> </li>
        }
    </ul>
}
else
{
    <pre> Unknown </pre>
}